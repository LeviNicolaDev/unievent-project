# .htaccess
<IfModule mod_rewrite.c>
    RewriteEngine On
    # Redireciona todas as requisições que não sejam arquivos ou diretórios
    # existentes para o seu index.php correto.
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ src/View/index.php [L]
</IfModule>